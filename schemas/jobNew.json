{
    "$id": "http://example.com/example.json",
    "$schema": "http://json-schema.org/draft-07/schema",
    "required": [
        "title",
        "company_handle"
    ],
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "maximum": 1.0
        },
        "company_handle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25
        }
    },
    "additionalProperties": false
}